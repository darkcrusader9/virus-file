		<script type="text/javascript">
        var ivrss_array	= new Array();
        var ivrss_obj	= '';
        var ivrss_scrollPos 	= '';
        var ivrss_numScrolls	= '';
        var ivrss_heightOfElm = '174';
        var ivrss_numberOfElm = '3';
		var ivrss_speed = '2';
		var ivrss_waitseconds = '2';
        var ivrss_scrollOn 	= 'true';
        function ivrss_createscroll() 
        {
            ivrss_array[0] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_self\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/Syl-2-Port-Car-Charger-SDL300096567-1-bad48-600x600-1.jpg\'></a></div>'; ivrss_array[1] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_blank\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/Samsung-Synthetic-Flip-Cover-Case-SDL093452042-1-03db4-600x600-1.jpg\'></a></div>'; ivrss_array[2] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_blank\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/71ayF3YyRwL._SL1500_-600x600.jpg\'></a></div>'; ivrss_array[3] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_self\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/51e6qvzBzqL._UX466_-1.jpg\'></a></div>'; ivrss_array[4] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_self\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/81ZtU43vLYL._UL1500_-600x600-1.jpg\'></a></div>'; ivrss_array[5] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_blank\' href=\'#\'><img border=\'0\' src=\'http://www.masinternationalcan.com/wp-content/uploads/2016/04/ere015s-ftcbazar-4-5-years-400x400-imaeexfhdy5rxycz-1.jpeg\'></a></div>'; ivrss_array[6] = '<div class=\'cas_div\' style=\'height:170px;padding:2px 0px 2px 0px;\'><a style=\'text-decoration:none\'  target=\'_self\' href=\'#\'><img border=\'0\' src=\'http://www.