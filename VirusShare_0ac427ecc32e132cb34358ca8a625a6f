            <script type="text/javascript">
               var DOMReady = function(a,b,c){b=document,c='addEventListener';b[c]?b[c]('DOMContentLoaded',a):window.attachEvent('onload',a)}
               DOMReady( function() {
                                    
                    Lib.setCookie('default_lang',  'ru', 'Sat, 01-Jan-2033 00:00:00 GMT', '/', '.www.meteoprog.com');                   
                    var aStoredCities = Lib.getCookieObject('last_visited_cities');
                    var sOut = ''; 
                    var cities_length=0;// фикс для того, чтобы если большие названия городов, не было ситуации, когда города не влазят в строчку
                    for (var i = 0; i < aStoredCities.length; i++){
                        cities_length += parseInt(aStoredCities[i].name.length);
                        if(cities_length>40)
                            break;
                        sOut += '<a href="' + aStoredCities[i].url + '"><span>' + aStoredCities[i].name + (i == (aStoredCities.length-1) ? '' : ', ') + '</span></a>';                        
                    }
                    $('#last_visited_cities').html(sOut);

                                            for (var i = 0; i < aStoredCities.length; i++) 
                            if (aStoredCities[i].name == "Сан-Себастьян-Тутла")
                            {
                                aStoredCities.splice(i, 1);
                                break;
                            }

                        while (aStoredCities.length > 4) {
                            aStoredCities.pop();
                        }

                        aStoredCities.unshift({name: "Сан-Себастьян-Тутла", url: "/ru/weather/SanSebastianTutla/"});
                        Lib.setCookieDays('last_visited_cities', JSON.stringify(aStoredCities), 100, '.' + location.hostname.replace('www.', ''),'/ru/');
                                    });
            </script>  
