welcome_guest();
function welcome_guest() {
var thedate;
var thehour;
thedate = new Date();
thehour = thedate.getHours();
if (thehour <12)
document.write("خوش آمديد ميهمان")
else if (thehour < 17)
document.write("خوش آمديد ميهمان")
else
document.write("خوش آمديد ميهمان")
}
